{
  "name": "linter-sourcekitten-syntax",
  "main": "./lib/init",
  "version": "0.1.0",
  "description": "Provide syntax information to Swift files using sourcekitten",
  "keywords": [
    "lint",
    "swift",
    "sourcekitten"
  ],
  "repository": "https://github.com/norio-nomura/linter-sourcekitten-syntax",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "os": [
    "darwin"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "dependencies": {
    "atom-linter": "^4.5.0",
    "atom-package-deps": "^4.0.1",
    "fs-plus": "2.x"
  },
  "package-deps": [
    "language-swift",
    "linter"
  ],
  "coffeelintConfig": {
    "indentation": {
      "level": "error",
      "value": 2
    },
    "max_line_length": {
      "value": 100,
      "level": "error"
    }
  }
}
